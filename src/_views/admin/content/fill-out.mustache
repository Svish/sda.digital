			data-name="{{filename}}"
			data-ext="{{fileext}}"
			data-time="{{time}}"
			data-type="{{mime.type}}"
			data-encoding="{{mime.encoding}}">{{path}}</div>
		

	</fieldset>

	<hr>
	<p>
		<button type="reset" data-bind="click: resetForm">Reset</button>
